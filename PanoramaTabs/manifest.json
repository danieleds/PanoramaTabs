{
  "manifest_version": 2,

  "name": "PanoramaTabs",
  "description": "The great \"Panorama\" feature of Firefox, on Chrome",
  "version": "1.0.1",

  "permissions": [
    "<all_urls>",
    "tabs",
    "storage"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "Group your tabs"
  }
}
